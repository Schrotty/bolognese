{
  "name": "bolognese",
  "version": "1.0.0",
  "description": "A simple pomodoro timer/ tool",
  "main": "index.js",
  "scripts": {
    "format": "tsfmt -r",
    "sass-watch": "sass --watch src/sass/style.scss dist/style/style.css",
    "copy-files": "copyfiles -f node_modules/feather-icons/dist/feather.min.js dist/script && copyfiles site.webmanifest dist/ && copyfiles -f src/js/worker.js dist/ && copyfiles -f node_modules/milligram/dist/* dist/style && copyfiles -f node_modules/animate.css/animate.min.css dist/style && copyfiles -f node_modules/normalize.css/normalize.css dist/style && copyfiles -f src/index.html dist",
    "build": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Schrotty/bolognese.git"
  },
  "keywords": [
    "pomodoro"
  ],
  "homepage": "https://github.com/Schrotty/bolognese",
  "author": "Schrotty",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.11.1",
    "copyfiles": "^2.2.0",
    "sass": "^1.26.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@types/howler": "^2.1.2",
    "animate.css": "^3.7.2",
    "feather-icons": "^4.28.0",
    "howler": "^2.1.3",
    "milligram": "^1.3.0",
    "normalize.css": "^8.0.1"
  }
}
